                            "title_description_raw": "title_description_raw",
                            "title.lemmas": "title_text",
                            "description.lemmas": "description_text"
                        }
                    }
                },
                {
                    "name": "concat",
                    "args": {
                        "input_fields": [
                            "title.lemmas",
